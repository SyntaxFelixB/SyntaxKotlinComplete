<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/home_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="164dp"
            android:text="HomeFragment"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/home_fragmentOne_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="44dp"
            android:text="Fragment 1"
            app:layout_constraintEnd_toEndOf="@+id/home_text"
            app:layout_constraintStart_toStartOf="@+id/home_text"
            app:layout_constraintTop_toBottomOf="@+id/home_text" />

        <Button
            android:id="@+id/home_fragmentTwo_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:text="Fragment 2"
            app:layout_constraintEnd_toEndOf="@+id/home_fragmentOne_button"
            app:layout_constraintHorizontal_bias="0.555"
            app:layout_constraintStart_toStartOf="@+id/home_fragmentOne_button"
            app:layout_constraintTop_toBottomOf="@+id/home_fragmentOne_button" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>